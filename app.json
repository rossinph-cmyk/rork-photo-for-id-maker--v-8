{
  "expo": {
    "name": "EZ.passport.ID.photos",
    "slug": "ezpassportidphotos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.ezpassportidphotos",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rossinphezpassportidphotos",
      "versionCode": 8,
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VISUAL_USER_SELECTED",
        "com.google.android.gms.permission.AD_ID",
        "RECORD_AUDIO",
        "android.permission.VIBRATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "READ_MEDIA_VIDEO",
        "READ_MEDIA_AUDIO",
        "ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow (EZ.passport.ID.photos) to access your photos"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow (EZ.passport.ID.photos) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow (EZ.passport.ID.photos) to access your photos.",
          "savePhotosPermission": "Allow (EZ.passport.ID.photos) to save photos.",
          "isAccessMediaLocationEnabled": false,
          "androidPermissions": [
            "READ_MEDIA_IMAGES",
            "READ_MEDIA_VISUAL_USER_SELECTED"
          ]
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "d0744897-d760-46cf-88fd-7094a8d05a03"
      }
    },
    "owner": "rossinph",
    "privacyPolicy": "https://docs.google.com/document/d/e/2PACX-1vSjsY9QbbhEQwk5j7Kp9Q7Q-6lNt_mPqKXQgDZCHj79bI_KO3L1Doq7CgVuVdM6yvG3vFqImYKiUrAC/pub"
  }
}